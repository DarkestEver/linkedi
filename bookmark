javascript:var $target = $('html,body'); setTimeout(  function()   { $target.animate({scrollTop: $target.height()}, 250); setTimeout(  function()   { $target.animate({scrollTop: $target.height()}, 500); setTimeout(  function()   { $target.animate({scrollTop: $target.height()}, 1000); setTimeout( function() { $(".result-context__connection-summary").remove(); $("figure").remove(); $(".result-lockup__actions").remove(); var table= []; $(".search-results__result-item").each(function( index , div) { var name_a = $(div).find(".result-lockup__name a"); var name = name_a.text(); var name_link = name_a.attr("href"); var position= $(div).find(".result-lockup__highlight-keyword").text().split("at")[0]; var company = $(div).find(".result-lockup__highlight-keyword a"); var c_name = company.text(); var c_link = company.attr("href"); table.push({ 'name' : name, 'name_link' : name_link , 'position':position, 'c_name':c_name, 'c_link':c_link }); $(div).hide(); }); var html_table = ""; html_table = html_table + "<table>"; $.each(table, function() { html_table = html_table + "<tr>"; $.each(this,function(key,val) {  html_table = html_table + "<td>"; html_table = html_table + val; html_table = html_table + "</td>"; }); html_table = html_table + "</tr>"; }); html_table = html_table + "</table>"; $(".search-results__result-list").append(html_table); }, 2500); }); }); }); var table= [];
